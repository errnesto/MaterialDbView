.graph
  .material-list
    margin     0
    padding    0
    list-style none
    
    .dot
      position        relative
      float           left
      margin          1px
      width           10px
      height          10px
      background-color white
      border-radius    10px
      &.document
        background-color #666
        border-radius    0
      &.unkown
        color            white
        background-color black
        border-radius    0
      &.gold
        background-color #F5A623
      &.tin
        background-color #50E3C2
        
    .tooltip
      position         absolute
      bottom           15px
      left             0
      width            200px
      height           70px
      background-color inherit
      padding          10px
      z-index          10